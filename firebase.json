{
	"hosting": {
		"public": "/",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"headers": [{
			"source": "app.js",
			"headers": [{
				"key": "Cache-Control",
				"value": "max-age=5"
			}]
		},{
			"source": "response.js",
			"headers": [{
				"key": "Cache-Control",
				"value": "max-age=5"
			}]
		},{
			"source": "app.css",
			"headers": [{
				"key": "Cache-Control",
				"value": "max-age=5"
			}]
		}]
	}
}
